# Editor config:
#   http://EditorConfig.org
# Complete list of properties:
#   https://github.com/editorconfig/editorconfig/wiki/EditorConfig-Properties

root = true

[*]
indent_style = tab
indent_size = tab
tab_width = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
# max_line_length = 120

[*.csv]
indent_style=space
indent_size=1

[{*.yml,*.yaml}]
indent_style=space
indent_size=2
